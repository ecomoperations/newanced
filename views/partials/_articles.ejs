 <div infinite-scroll='catFactory.nextPage("{{filter1.category}}")' infinite-scroll-disabled='catFactory.busy' infinite-scroll-distance='1'>
			    <div class="col-xs-6" >
			    <div class="card card-signup padding-top" >
					<form class="form" method="" action="">
						<div class="header header-info text-center">
							<h4>Liberal-Leaning</h4>
							
						</div>
						
					</form>
				</div>
			    <div ng-repeat='post in catFactory.items | filter:filter1'>

			    	<div class="card card-blog">
						
						<div class="card-image">
							<a href="/posts/{{post._id}}" target="_blank">
								<img class="img img-raised" src="{{post.photo}}" ng-show="post.photo">
							</a>
						</div>
						<div class="content">
							<h6 class="category text-info-liberal">{{categoryFunction(post)}}</h6>
							<h4 class="card-title">
								<a href="{{post.link}}" target="_blank">{{post.title}}</a>
							</h4>
							<p class="card-description"  >
								{{post.content | limitTo:250 | stripTags }}<a href="/posts/{{post._id}}" target="_blank"> Read More </a>
							</p>
						</div>
					</div>

			  
			      <div style='clear: both;'></div>
			    </div>
			    <div ng-show='catFactory.busy'>Loading data...</div>
			  </div>
			  </div>

			  <div infinite-scroll='catFactory.nextPage("{{filter2.category}}")' infinite-scroll-disabled='catFactory.busy' infinite-scroll-distance='1'>
			    <div class="col-xs-6" >
			    <div class="card card-signup padding-top">
							<form class="form" method="" action="">
								<div class="header header-primary text-center">
									<h4>Conservative-Leaning</h4>
									
								</div>
								
							</form>
						</div>
			    <div ng-repeat='post in catFactory.items | filter:filter2'>

			    	<div class="card card-blog">
						
						<div class="card-image">
							<a href="/posts/{{post._id}}" target="_blank">
								<img class="img img-raised" src="{{post.photo}}" ng-show="post.photo">
							</a>
						</div>
						<div class="content">
							<h6 class="category text-info-conservative">{{categoryFunction(post)}}</h6>
							<h4 class="card-title">
								<a href="{{post.link}}" target="_blank">{{post.title}}</a>
							</h4>
							<p class="card-description"  >
								{{post.content | limitTo:250 | stripTags }}<a href="/posts/{{post._id}}" target="_blank"> Read More </a>
							</p>
						</div>
					</div>

			   <!--    <span class='score'>{{item.score}}</span>
			      <span class='title'>
			        <a ng-href='{{item.url}}' target='_blank'>{{item.title}}</a>
			      </span>
			      <small>by {{item.author}} -
			        <a ng-href='http://reddit.com{{item.permalink}}' target='_blank'>{{item.num_comments}} comments</a>
			      </small> -->
			      <div style='clear: both;'></div>
			    </div>
			    <div ng-show='catFactory.busy'>Loading data...</div>
			  </div>
			  </div>
